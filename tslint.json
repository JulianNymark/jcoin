{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "backtick",
            "always"
        ],
        "no-console": [
            false
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "object-literal-sort-keys": [
            false
        ]
    },
    "rulesDirectory": []
}